package BOOKINGTOUR.entity;

import java.io.Serializable;

import javax.persistence.Embeddable;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;


	@Embeddable
	public class PK implements Serializable {
		@ManyToOne()
		@JoinColumn(name="MATOUR")
		private Tour tour1;
		@ManyToOne()
		@JoinColumn(name="MADIEMDULICH")
		private DiemDuLich diemDuLich;
		public Tour getTour1() {
			return tour1;
		}
		public void setTour1(Tour tour1) {
			this.tour1 = tour1;
		}
		public DiemDuLich getDiemDuLich() {
			return diemDuLich;
		}
		public void setDiemDuLich(DiemDuLich diemDuLich) {
			this.diemDuLich = diemDuLich;
		}}

