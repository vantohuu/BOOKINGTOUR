package BOOKINGTOUR.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="BOOKINGTOUR")
public class BookingTour {
	@Id
	@Column(name="MABK")
	private int maBK;
	@Column(name="TGBD")
	private  String tGBD;
	
	@Column(name="TGKT")
	private String tGKT;
	
	@Column(name="MAXNL")
	private  String maxNL;
	@Column(name="MAXTE")
	private String maxTE;
	@Column(name="MAXTN")
	private  String maxtn;
	@Column(name="MINNL")
	private String minNL;
	@Column(name="MINTE")
	private  String minTE;
	@Column(name="MINTN")
	private String minTN;
	@Column(name="TRANGTHAI")
	private  String trangThai;
	@Column(name="GHICHU")
	private String ghiChu;
	@Column(name="MATOUR")
	private  String maTour;
	@Column(name="MALT")
	private  String maLT;

}
