package test;

import java.util.List;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.OneToMany;
import javax.persistence.Table;


@Entity
@Table(name="CTVE")
public class CTVe {
	@Id
	@Column(name="IDCTVE")
	private  int iDCTVe;
	
	@ManyToOne()
	@JoinColumn(name="MAKH")
	private KhachHang khachHang;
	
	@ManyToOne()
	@JoinColumn(name="MAVE")
	private VeTour veTour;
	
	@ManyToOne()
	@JoinColumn(name="MAKM")
	private KhuyenMai khuyenMai;
	
	@OneToMany(mappedBy = "cTve", fetch = FetchType.EAGER)
	private List<NhanVien> nhanVienXN;
	
	@OneToMany(mappedBy = "cTve1", fetch = FetchType.EAGER)
	private List<NhanVien> nhanVienHuy;

	public int getiDCTVe() {
		return iDCTVe;
	}

	public void setiDCTVe(int iDCTVe) {
		this.iDCTVe = iDCTVe;
	}

	public KhachHang getKhachHang() {
		return khachHang;
	}

	public void setKhachHang(KhachHang khachHang) {
		this.khachHang = khachHang;
	}

	public VeTour getVeTour() {
		return veTour;
	}

	public void setVeTour(VeTour veTour) {
		this.veTour = veTour;
	}

	public KhuyenMai getKhuyenMai() {
		return khuyenMai;
	}

	public void setKhuyenMai(KhuyenMai khuyenMai) {
		this.khuyenMai = khuyenMai;
	}

	public List<NhanVien> getNhanVienXN() {
		return nhanVienXN;
	}

	public void setNhanVienXN(List<NhanVien> nhanVienXN) {
		this.nhanVienXN = nhanVienXN;
	}

	public List<NhanVien> getNhanVienHuy() {
		return nhanVienHuy;
	}

	public void setNhanVienHuy(List<NhanVien> nhanVienHuy) {
		this.nhanVienHuy = nhanVienHuy;
	}
	

}
